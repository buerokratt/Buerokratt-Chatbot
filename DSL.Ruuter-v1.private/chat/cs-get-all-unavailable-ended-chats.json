{
  "action_type": "BLOGIC",
  "destination": [
    {
      "cs_get_all_unavailable_ended_chats": {
        "verify": {
          "response": {
            "ok": "proceed",
            "nok": "stop"
          }
        },
        "proceed": {
          "method": "post",
          "endpoint": "{resql_url}/get-cs-all-unavailable-ended-chats",
          "post_body_parameters": {
            "check_roles": {
              "method": "post",
              "endpoint": "{ruuter_url}/match_input",
              "post_body_parameters": {
                "userinfo": {
                  "cookies": ["customJwtCookie"],
                  "method": "post",
                  "endpoint": "{tim_url}/jwt/custom-jwt-userinfo",
                  "post_body_struct": "customJwtCookie",
                  "response": {
                    "ok": "proceed",
                    "nok": "stop"
                  }
                }
              },
              "post_body_struct": {
                "input": {
                  "authorities": "{$.userinfo$.authorities}"
                },
                "validate_against": {
                  "authorities": [
                    "ROLE_ADMINISTRATOR",
                    "ROLE_CUSTOMER_SUPPORT_AGENT",
                    "ROLE_SERVICE_MANAGER"
                  ]
                },
                "validation_type": "MUST_MATCH_ANY"
              },
              "response": {
                "ok": "proceed",
                "nok": "stop"
              }
            }
          },
          "post_body_struct": {
            "start": "{#.startDate}",
            "end": "{#.endDate}"
          }
        },
        "response": {
          "ok": "proceed",
          "nok": "stop"
        }
      }
    }
  ]
}
