{
  "action_type": "BLOGIC",
  "destination": {
    "blacklist_client_custom_jwt": {
      "verify": {
        "response": {
          "ok": "proceed",
          "nok": "proceed"
        }
      },
      "proceed": {
        "cookies": [
          "clientCustomJwtCookie"
        ],
        "method": "post",
        "endpoint": "{tim_url}/jwt/custom-jwt-blacklist",
        "post_body_struct": "clientCustomJwtCookie",
        "response_header": {
          "Set-Cookie": {
            "name": "clientCustomJwtCookie",
            "httpOnly": true,
            "maxAge": 0,
            "domain": "localhost",
            "setSecure": true,
            "value": null
          }
        },
        "response": {
          "ok": "proceed",
          "nok": "stop"
        }
      }
    }
  }
}
